    "mocha-junit-reporter": "^2.0.2",
    "nyc": "^15.1.0",
    "nock": "^13.2.4",
    "nodemon": "^3.1.0",
    "prettier": "^2.6.2",
    "pre-commit": "^1.2.2",
    "sinon": "^17.0.0",
    "supertest": "^6.2.3"
