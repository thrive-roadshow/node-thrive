    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "express-correlation-id": "^2.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.8.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongodb": "^5.9.2",
